{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Next.js tsconfig",
  "extends": "./tsconfig-base.json",
  "compilerOptions": {
    "lib": ["DOM", "DOM.Iterable", "ES2022"],
    "jsx": "react-jsx",
    "moduleResolution": "Bundler",
    "module": "Preserve",
    "noEmit": true,
    "skipLibCheck": true
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx"]
}
